def find_max_min(arr):
    max_val = arr[0]
    min_val = arr[0]
    for i in range(1, len(arr)):
        if arr[i] > max_val:
            max_val = arr[i]
        elif arr[i] < min_val:
            min_val = arr[i]
    return max_val, min_val

arr = [5, 2, 8, 1, 9]
print("Maximum:", find_max_min(arr)[0])
print("Minimum:", find_max_min(arr)[1])
